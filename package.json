    "@ledgerhq/hw-transport-webhid": "^6.28.6",
    "@ledgerhq/hw-transport-webusb": "^6.28.6",
    "bs58": "^4.0.1",
    "platform": "^1.3.6"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "browserslist": [
