{
  "name": "near-ledger-js",
  "version": "0.2.1",
  "description": "Connect to NEAR Ledger app from browser",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "parcel --https demo/index.html",
    "test": "parcel build demo/index.html"
  },
  "devDependencies": {
    "@ledgerhq/logs": "^6.12.0",
    "buffer": "^6.0.0",
